{
  "manifest_version": 3,
  "name": "엔트리-Klets",
  "version": "1.0",
  "description": "탐험하기 채팅에 유용한 기능을 추가해줍니다.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://space.playentry.org/world/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "엔트리-Klets"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 